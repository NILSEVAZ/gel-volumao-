<script>
var elGlucosamineStudyChart = document.getElementById('glucosamineStudyChart');
    var glucosamineStudyChart = new Chart(elGlucosamineStudyChart, {
        type: 'bar',
        data: {
            labels: ['Glucosamine', 'Placebo'],
            datasets: [{
                label: '% Pain Reduction',
                data: [19, 33],
                backgroundColor: [
                    'rgba(192, 0, 0, 0.7)',
                    'rgba(24, 140, 251, 0.7)'
                ],
                borderColor: [
                    'rgba(192, 0, 0, 1)',
                    'rgba(24, 140, 251, 1)'
                ],
                borderWidth: 1
            }]
        },
        options: {
            legend: {
                labels: {
                    fontColor: 'white'
                }
            },
            scales: {
                xAxes: [{
                    ticks: {
                        fontColor: '#fff'
                    },
                    gridLines: {
                        color: 'rgba(255, 255, 255, 1.0)',
                        zeroLineColor: 'rgba(255, 255, 255, 1.0)'
                    }
                }],
                yAxes: [{
                    ticks: {
                        fontColor: '#fff',
                        beginAtZero: true,
                        callback: function(value) {
                            return value + '%';
                        }
                    },
                    gridLines: {
                        color: 'rgba(255, 255, 255, 1.0)',
                        zeroLineColor: 'rgba(255, 255, 255, 1.)'
                    }
                }]
            },
            tooltips: {
                callbacks: {
                    label: function(tooltipItem, data) {
                        return tooltipItem.yLabel + '%';
                    }
                }
            }
        }
    });
</script>  
